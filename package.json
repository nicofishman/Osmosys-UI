{
  "name": "storybook",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "storybook": "NODE_OPTIONS='--openssl-legacy-provider' start-storybook -p 7007",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "expo": "~48.0.6",
    "expo-status-bar": "~1.4.4",
    "jest": "^29.2.1",
